      "id": "generate-ai-advice",
      "name": "Generate AI Advice",
      "type": "@n8n/n8n-nodes-langchain.openAi",
      "typeVersion": 1.3,
      "position": [1120, 300],
      "credentials": {
        "openAiApi": {
          "id": "openai-nutrichecker",
          "name": "OpenAI NutriChecker"
        }
      }
